<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.TbConfigurationMapper">

    <update id="updateRider">
        update tb_configuration
        set riderSwitch=#{riderSwitch}
    </update>

    <select id="selectAllConfig" resultType="com.example.demo.entity.TbConfiguration">
        select * from tb_configuration
    </select>
</mapper>